<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="All Automation Practice Tests" parallel="tests" thread-count="2">


    <test name="Negative Login Tests F">
        <parameter name="browser" value="FIREFOX"/>
        <classes>
            <class name="tests.FailedLoginTests"/>
        </classes>
    </test>

    <test name="Negative Login Tests C">
        <parameter name="browser" value="CHROME"/>
        <classes>
            <class name="tests.FailedLoginTests"/>
        </classes>
    </test>

    <test name="Negative Login Tests IE">
        <parameter name="browser" value="IE"/>
        <classes>
            <class name="tests.FailedLoginTests"/>
        </classes>
    </test>

    <test name="Positive Login Tests">
        <parameter name="browser" value="CHROME"/>
        <classes>
            <class name="tests.PositiveLoginTests"/>
        </classes>
    </test>

    <test name="Positive Login Tests F">
        <parameter name="browser" value="FIREFOX"/>
        <classes>
            <class name="tests.PositiveLoginTests"/>
        </classes>
    </test>

    <test name="Positive Login Tests IE">
        <parameter name="browser" value="IE"/>
        <classes>
            <class name="tests.PositiveLoginTests"/>
        </classes>
    </test>


</suite>